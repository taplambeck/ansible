#Ansible Hosts File. Location /etc/ansible/hosts

[wan_routers]
10.10.10.82

[external_switches]
[dmz_switches]
[mdf_switches]
[idf_switches]

[cisco:devices]
wan_routers
external_switches
dmz_switches
mdf_switches
idf_switches

[cisco:vars]
ansible_connection=ssh
ansible_user=cisco
ansible_ssh_pass=cisco

[routers:children]
wan_routers

[routers:vars]
ansible_connection=ssh
ansible_user=cisco
ansible_ssh_pass=cisco

[switches:children]
external_switches
dmz_switches
mdf_switches
idf_switches

[switches:vars]
ansible_connection=ssh
ansible_user=cisco
ansible_ssh_pass=cisco
